<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  
  <metadata>
    <id>FbonizziMonoGame</id>
    <version>1.0.7-alpha</version>
    <title>FbonizziMonoGame</title>
    <authors>Francesco Bonizzi</authors>
    <owners>Francesco Bonizzi</owners>
    <description>Some libraries that I built over MonoGame Framework to make my games</description>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectUrl>https://github.com/FrancescoBonizzi/FbonizziMonoGame</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/FrancescoBonizzi/FbonizziMonoGame/master/logo-64x64.png</iconUrl>
    <copyright>Copyright 2018</copyright>
    <tags>monogame francesco bonizzi games videogames android UWP UAP xamarin</tags>

    <dependencies>
      <group targetFramework="monoandroid81">
        <dependency id="MonoGame.Framework.Android" version="3.7.0.0" />
      </group>

      <group targetFramework="UAP10">
        <dependency id="MonoGame.Framework.WindowsUniversal" version="3.7.0.0" />
      </group>

      <group targetFramework="net462">
        <dependency id="MonoGame.Framework.WindowsDX" version="3.7.0.0" />
      </group>

      <group targetFramework="portable-net45+win8+wpa81">
        <dependency id="MonoGame.Framework.Portable" version="3.7.0.0" />
      </group>

      <group targetFramework="netstandard2.0">
        <dependency id="NETStandard.Library" version="2.0.0" />
        <dependency id="MonoGame.Framework.Portable" version="3.7.0.0" />
      </group>
      
    </dependencies>
    
  </metadata>
   
  <files>
    <file src="FbonizziMonoGame\bin\Release\FbonizziMonoGame.*" target="lib\net45" />
    <file src="FbonizziMonoGame\bin\Release\FbonizziMonoGame.*" target="lib\uap10" />
    <file src="FbonizziMonoGame\bin\Release\FbonizziMonoGame.*" target="lib\monoandroid81" />
    <file src="FbonizziMonoGame\bin\Release\FbonizziMonoGame.*" target="lib\portable-net45+win8+wpa81" />
    <file src="FbonizziMonoGame\bin\Release\FbonizziMonoGame.*" target="lib\netstandard2.0" />
  </files>
  
</package>